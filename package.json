{
	"name": "george-kayzer",
	"version": "1.0.0",
	"description": "George Kayzer - The Personal Page",
	"main": "server/app.js",
	"scripts": {
		"start": "concurrently --kill-others \"npm run _server:run:debug\" \"ng serve --aot=true --progress=false --proxy-config proxy.conf.json\"",
		"lint:client": "ng lint",
		"lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
		"test:client": "ng test",
		"e2e:client": "ng e2e",
		"build": "ng build --prod --sm=false --aot=true --output-path=dist/client && npm run _server:build",
		"_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
		"_server:run:debug": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon --inspect-brk dist/server/bin/www.js\" ",
		"_server:build": "tsc -p ./server",
		"postinstall": "npm run build"
	},
	"author": "George Kayzer",
	"repository": {
		"url": "https://github.com/Niaro/george-kayzer"
	},
	"private": true,
	"dependencies": {
		"@angular/cli": "1.2.0",
		"@angular/common": "4.2.6",
		"@angular/compiler": "4.2.6",
		"@angular/compiler-cli": "4.2.6",
		"@angular/core": "4.2.6",
		"@angular/forms": "4.2.6",
		"@angular/http": "4.2.6",
		"@angular/platform-browser": "4.2.6",
		"@angular/platform-browser-dynamic": "4.2.6",
		"@angular/router": "4.2.6",
		"@ngrx/core": "1.2.0",
		"@ngrx/effects": "2.0.3",
		"@ngrx/store": "2.2.2",
		"@ngrx/store-devtools": "3.2.4",
		"@types/body-parser": "1.16.4",
		"@types/compression": "0.0.33",
		"@types/cors": "2.8.1",
		"@types/express": "4.0.36",
		"@types/jasmine": "2.5.53",
		"@types/jsonwebtoken": "7.2.2",
		"@types/node": "8.0.9",
		"@types/uuid": "3.0.0",
		"body-parser": "1.17.2",
		"compression": "1.6.2",
		"cookie-parser": "1.4.3",
